{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "jsonrpc": {
      "type": "string"
    },
    "result": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "processed_hardforks": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "last_hardfork": {
          "type": "integer"
        },
        "current_hardfork_version": {
          "type": "string",
          "pattern": "[0-9]+(\\.[0-9]+){2}"
        },
        "next_hardfork": {
          "type": "string",
          "pattern": "[0-9]+(\\.[0-9]+){2}"
        },
        "next_hardfork_time": {
          "type": "string"
        }
      },
      "required": [
        "current_hardfork_version",
        "id",
        "last_hardfork",
        "next_hardfork",
        "next_hardfork_time",
        "processed_hardforks"
      ]
    },
    "id": {
      "type": "integer"
    }
  },
  "required": [
    "id",
    "jsonrpc",
    "result"
  ]
}

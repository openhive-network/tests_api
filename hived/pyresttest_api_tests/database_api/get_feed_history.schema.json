                "required": [
                  "amount",
                  "nai",
                  "precision"
                ]
              },
              "quote": {
                "type": "object",
                "properties": {
                  "amount": {
                    "type": "string",
                    "pattern": "[1-9][0-9]*"
                  },
                  "precision": {
                    "type": "integer",
                  },
                  "nai": {
                    "type": "string",
                    "pattern": "^@@[0-9]+$"
                  }
                },
                "required": [
                  "amount",
                  "nai",
                  "precision"
                ]
              }
            },
            "required": [
              "base",
              "quote"
            ]
          }
        }
      },
      "required": [
        "current_median_history",
        "id",
        "price_history"
      ]

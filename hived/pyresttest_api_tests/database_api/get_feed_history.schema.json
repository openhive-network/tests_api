{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "jsonrpc": {
      "type": "string"
    },
    "result": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "current_median_history": {
          "type": "object",

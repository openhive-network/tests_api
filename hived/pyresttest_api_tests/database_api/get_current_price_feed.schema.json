{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "jsonrpc": {
      "type": "string"
    },
    "result": {
      "type": "object",
      "properties": {
        "base": {
          "type": "object",
          "properties": {
            "amount": {
              "type": "string",
              "pattern": "[1-9][0-9]*"
            },
            "precision": {
              "type": "integer",
            },
            "nai": {
              "type": "string",
              "pattern": "^@@[0-9]+$"
            }
          },
          "required": [
            "amount",
            "nai",
            "precision"
          ]
        },
        "quote": {
          "type": "object",
          "properties": {
            "amount": {
              "type": "string",
              "pattern": "[1-9][0-9]*"
            },
            "precision": {
              "type": "integer",
            },
            "nai": {
              "type": "string",
              "pattern": "^@@[0-9]+$"
            }
          },
          "required": [
            "amount",
            "nai",
            "precision"
          ]
        }
      },
      "required": [
        "base",
        "quote"
      ]
    },
    "id": {
      "type": "integer"
    }
  },
  "required": [
    "id",
    "jsonrpc",
    "result"
  ],
  "additionalProperties": false
}
